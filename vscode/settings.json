{
  "[cpp]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "[python]": {
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit",
      "source.organizeImports": "explicit"
    },
    "editor.defaultFormatter": "charliermarsh.ruff",
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "editor.wordWrapColumn": 80
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "black-formatter.args": ["--line-length=80 "],
  "clangd.arguments": ["-function-arg-placeholders=false"],
  "diffEditor.renderSideBySide": false,
  "editor.bracketPairColorization.enabled": true,
  "editor.cursorStyle": "line",
  "editor.cursorSurroundingLines": 2,
  "editor.cursorSurroundingLinesStyle": "all",
  "editor.folding": false,
  "editor.fontFamily": "'JetBrains Mono', Consolas, 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.fontLigatures": true,
  "editor.fontSize": 12,
  "editor.inlayHints.enabled": "off",
  "editor.insertSpaces": true,
  "editor.lineNumbers": "off",
  "editor.matchBrackets": "near",
  "editor.quickSuggestions": {
    "comments": false,
    "other": true,
    "strings": false
  },
  "editor.renderFinalNewline": "off",
  "editor.renderWhitespace": "none",
  "editor.rulers": [80],
  "editor.stickyScroll.enabled": true,
  "editor.suggest.insertMode": "replace",
  "editor.suggest.localityBonus": true,
  "editor.suggestSelection": "first",
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "[Default Dark+]": {
      "comments": "#13c100",
      "functions": "#ffa600",
      "numbers": "#fffb00",
      "textMateRules": [
        {
          "scope": ["variable"],
          "settings": {
            "foreground": "#ffffff"
          }
        },
        {
          "scope": [
            "variable.other.property",
            "variable.other.object.property"
          ],
          "settings": {
            "foreground": "#d1baec"
          }
        },
        {
          "scope": ["meta.function-call"],
          "settings": {
            "foreground": "#ffa600"
          }
        },
        {
          "scope": ["constant", "constant.language"],
          "settings": {
            "foreground": "#fffb00"
          }
        },
        {
          "scope": ["meta.attribute"],
          "settings": {
            "foreground": "#aa00ff"
          }
        },
        {
          "scope": ["keyword.operator", "punctuation", "meta.brace"],
          "settings": {
            "foreground": "#00f2ff"
          }
        },
        {
          "scope": ["keyword", "keyword.control", "keyword.other.using"],
          "settings": {
            "foreground": "#3664ff"
          }
        },
        {
          "scope": ["storage.type", "storage.class"],
          "settings": {
            "foreground": "#6d4dfd"
          }
        }
      ],
      "types": "#aa00ff"
    }
  },
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.wordWrap": "off",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 1,
  "extensions.experimental.affinity": {
    "vscodevim.vim": 1
  },
  "extensions.ignoreRecommendations": false,
  "files.associations": {
    "*.pi": "python",
    "*.sdl": "sql-bigquery",
    "*.textproto": "plaintext",
    "BUILD": "bazel"
  },
  "files.autoSave": "onWindowChange",
  "files.exclude": {
    "**/__pycache__ ": true,
    "**/.aider.*": true,
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.hypothesis": true,
    "**/.mypy_cache": true,
    "**/.project": true,
    "**/.pytest_cache": true,
    "**/.ruff_cache": true,
    "**/.settings": true,
    "**/.venv": true,
    "**/*.egg-info": true
  },
  "files.hotExit": "off",
  "files.insertFinalNewline": true,
  "files.refactoring.autoSave": true,
  "files.simpleDialog.enable": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.defaultCloneDirectory": "~/repos",
  "git.detectSubmodules": false,
  "git.detectSubmodulesLimit": 9999,
  "git.path": "/usr/bin/git",
  "javascript.format.semicolons": "insert",
  "linkme.rules": [
    {
      "pattern": "([a-z]{1,4})/([^)]+)",
      "url": "http://\\1/\\2"
    }
  ],
  "merge-conflict.autoNavigateNextConflict.enabled": true,
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "problems.showCurrentInStatus": true,
  "python.analysis.enablePytestSupport": true,
  "python.analysis.typeCheckingMode": "standard",
  "python.languageServer": "Pylance",
  "ruff.nativeServer": "on",
  "search.quickOpen.history.filterSortOrder": "recency",
  "terminal.integrated.smoothScrolling": true,
  "typescript.format.semicolons": "insert",
  "update.mode": "manual",
  "vim.easymotion": true,
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-b>": false,
    "<C-c>": false,
    "<C-d>": false,
    "<C-e>": false,
    "<C-f>": false,
    "<C-h>": false,
    "<C-j>": true,
    "<C-k>": true,
    "<C-o>": false,
    "<C-q>": false,
    "<C-t>": false,
    "<C-v>": false,
    "<C-w>": false
  },
  "vim.highlightedyank.enable": true,
  "vim.hlsearch": false,
  "vim.incsearch": true,
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "after": ["<Esc>"],
      "before": [",", "."]
    }
  ],
  "vim.leader": "<space>",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "after": ["o", "<Esc>", "k"],
      "before": ["z", "j"]
    },
    {
      "after": ["O", "<Esc>", "j"],
      "before": ["z", "k"]
    },
    {
      "after": ["1", "5", "g", "k"],
      "before": ["<c-k>"]
    },
    {
      "after": ["1", "5", "g", "j"],
      "before": ["<c-j>"]
    },
    {
      "after": ["y", "$"],
      "before": ["Y"]
    },
    {
      "before": [";"],
      "commands": ["workbench.action.showCommands"]
    },
    {
      "before": ["<leader>", "t"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<leader>", "j"],
      "commands": ["workbench.action.navigateDown"]
    },
    {
      "before": ["<leader>", "k"],
      "commands": ["workbench.action.navigateUp"]
    },
    {
      "before": ["<leader>", "l"],
      "commands": ["workbench.action.navigateRight"]
    },
    {
      "before": ["<leader>", "h"],
      "commands": ["workbench.action.navigateLeft"]
    },
    {
      "before": ["<leader>", "b"],
      "commands": ["extension.vim_ctrl+o"]
    },
    {
      "before": ["<leader>", "f"],
      "commands": ["extension.vim_ctrl+i"]
    },
    {
      "before": ["<leader>", "s", "h"],
      "commands": ["togglehs.toggleHS"]
    },
    {
      "after": ["g", "d"],
      "before": ["<leader>", "g"],
      "doc": "this is go-to-definition; interacts with vsvim jump list better"
    },
    {
      "before": ["<leader>", "p", "c"],
      "commands": ["editor.action.revealDeclaration"]
    },
    {
      "before": ["<leader>", "q"],
      "commands": ["workbench.action.closeWindow"]
    },
    {
      "before": ["<leader>", "w"],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["<leader>", "m"],
      "commands": ["editor.action.formatDocument"]
    },
    {
      "before": ["<leader>", "p", "d"],
      "commands": ["editor.action.peekDefinition"]
    },
    {
      "before": ["<leader>", "p", "r"],
      "commands": ["editor.action.referenceSearch.trigger"]
    },
    {
      "before": ["<leader>", "c", "s"],
      "commands": ["codesearch.openCurrentFile"]
    },
    {
      "after": [],
      "before": ["u"],
      "commands": [
        {
          "args": [],
          "command": "undo"
        }
      ]
    },
    {
      "after": [],
      "before": ["<C-r>"],
      "commands": [
        {
          "args": [],
          "command": "redo"
        }
      ]
    }
  ],
  "vim.sneak": true,
  "vim.useCtrlKeys": true,
  "vim.useSystemClipboard": true,
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "after": ["<Esc>"],
      "before": [",", "."]
    },
    {
      "after": ["1", "5", "g", "k"],
      "before": ["<c-k>"]
    },
    {
      "after": ["1", "5", "g", "j"],
      "before": ["<c-j>"]
    }
  ],
  "vsicons.dontShowNewVersionMessage": true,
  "window.commandCenter": false,
  "window.newWindowDimensions": "fullscreen",
  "window.openFilesInNewWindow": "default",
  "window.restoreFullscreen": true,
  "window.restoreWindows": "none",
  "window.title": "${rootName}${separator}${dirty}${activeEditorShort}",
  "window.titleBarStyle": "custom",
  "workbench.colorCustomizations": {
    "editor.background": "#000000",
    "editor.foreground": "#ffffff"
  },
  "workbench.editor.showTabs": "single",
  "workbench.iconTheme": "vscode-icons",
  "workbench.layoutControl.enabled": false,
  "workbench.activityBar.location": "hidden"
}
