{
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[python]": {
    "editor.defaultFormatter": "google.g3",
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "clangd.semanticHighlighting": false,
  "clangd.arguments": ["-function-arg-placeholders=false"],
  "diffEditor.renderSideBySide": false,
  "editor.cursorSurroundingLines": 2,
  "editor.cursorSurroundingLinesStyle": "all",
  "editor.folding": false,
  "editor.fontFamily": "'JetBrains Mono', Consolas, 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.fontLigatures": true,
  "editor.fontSize": 12,
  "editor.lineNumbers": "off",
  "editor.matchBrackets": "near",
  "editor.quickSuggestions": {
    "comments": false,
    "other": true,
    "strings": false
  },
  "editor.renderFinalNewline": false,
  "editor.renderWhitespace": "none",
  "editor.rulers": [80],
  "editor.suggest.localityBonus": true,
  "editor.suggest.maxVisibleSuggestions": 10,
  "editor.suggestSelection": "first",
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "[Default Dark+]": {
      "comments": "#13c100",
      "functions": "#ffa600",
      "numbers": "#fffb00",
      "textMateRules": [
        {
          "scope": ["variable"],
          "settings": {
            "foreground": "#ffffff"
          }
        },
        {
          "scope": [
            "variable.other.property",
            "variable.other.object.property"
          ],
          "settings": {
            "foreground": "#d1baec"
          }
        },
        {
          "scope": ["meta.function-call"],
          "settings": {
            "foreground": "#ffa600"
          }
        },
        {
          "scope": ["constant", "constant.language"],
          "settings": {
            "foreground": "#fffb00"
          }
        },
        {
          "scope": ["meta.attribute"],
          "settings": {
            "foreground": "#aa00ff"
          }
        },
        {
          "scope": ["keyword.operator", "punctuation", "meta.brace"],
          "settings": {
            "foreground": "#00f2ff"
          }
        },
        {
          "scope": ["keyword", "keyword.control", "keyword.other.using"],
          "settings": {
            "foreground": "#3664ff"
          }
        },
        {
          "scope": ["storage.type", "storage.class"],
          "settings": {
            "foreground": "#6d4dfd"
          }
        }
      ],
      "types": "#aa00ff"
    }
  },
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 0,
  "extensions.ignoreRecommendations": false,
  "files.associations": {
    "*.pi": "python",
    "*.sdl": "sql-bigquery",
    "*.textproto": "plaintext",
    "BUILD": "bazel"
  },
  "files.autoSave": "onWindowChange",
  "files.exclude": {
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.project": true,
    "**/.settings": true
  },
  "files.hotExit": "off",
  "files.insertFinalNewline": true,
  "files.simpleDialog.enable": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.defaultCloneDirectory": "~/repos",
  "git.path": "/usr/bin/git",
  "javascript.format.semicolons": "insert",
  "linkme.rules": [
    {
      "pattern": "([a-z]{1,4})/([^)]+)",
      "url": "http://\\1/\\2"
    }
  ],
  "merge-conflict.autoNavigateNextConflict.enabled": true,
  "problems.showCurrentInStatus": true,
  "python.formatting.blackArgs": ["--line-length", "80"],
  "python.formatting.provider": "black",
  "python.jediEnabled": false,
  "search.quickOpen.history.filterSortOrder": "recency",
  "typescript.format.semicolons": "insert",
  "update.mode": "manual",
  "vim.easymotion": true,
  "vim.easymotionMarkerFontFamily": "'JetBrains Mono', Consolas, 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-b>": false,
    "<C-c>": false,
    "<C-d>": false,
    "<C-e>": false,
    "<C-f>": false,
    "<C-h>": false,
    "<C-j>": true,
    "<C-k>": true,
    "<C-o>": false,
    "<C-q>": false,
    "<C-t>": false,
    "<C-v>": false,
    "<C-w>": false
  },
  "vim.highlightedyank.enable": true,
  "vim.hlsearch": false,
  "vim.incsearch": true,
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "after": ["<Esc>"],
      "before": [",", "."]
    }
  ],
  "vim.leader": "<space>",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "after": ["o", "<Esc>", "k"],
      "before": ["z", "j"]
    },
    {
      "after": ["O", "<Esc>", "j"],
      "before": ["z", "k"]
    },
    {
      "after": ["1", "5", "g", "k"],
      "before": ["<c-k>"]
    },
    {
      "after": ["1", "5", "g", "j"],
      "before": ["<c-j>"]
    },
    {
      "after": ["y", "$"],
      "before": ["Y"]
    },
    {
      "before": [";"],
      "commands": ["workbench.action.showCommands"]
    },
    {
      "before": ["<leader>", "t"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<leader>", "j"],
      "commands": ["workbench.action.navigateDown"]
    },
    {
      "before": ["<leader>", "k"],
      "commands": ["workbench.action.navigateUp"]
    },
    {
      "before": ["<leader>", "l"],
      "commands": ["workbench.action.navigateRight"]
    },
    {
      "before": ["<leader>", "h"],
      "commands": ["workbench.action.navigateLeft"]
    },
    {
      "before": ["<leader>", "b"],
      "commands": ["extension.vim_ctrl+o"]
    },
    {
      "before": ["<leader>", "f"],
      "commands": ["extension.vim_ctrl+i"]
    },
    {
      "before": ["<leader>", "s", "h"],
      "commands": ["togglehs.toggleHS"]
    },
    {
      "after": ["g", "d"],
      "before": ["<leader>", "g"],
      "doc": "this is go-to-definition; interacts with vsvim jump list better"
    },
    {
      "before": ["<leader>", "p", "c"],
      "commands": ["editor.action.revealDeclaration"]
    },
    {
      "before": ["<leader>", "q"],
      "commands": ["workbench.action.closeWindow"]
    },
    {
      "before": ["<leader>", "w"],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["<leader>", "m"],
      "commands": ["editor.action.formatDocument"]
    },
    {
      "before": ["<leader>", "p", "d"],
      "commands": ["editor.action.peekDefinition"]
    },
    {
      "before": ["<leader>", "p", "r"],
      "commands": ["editor.action.referenceSearch.trigger"]
    },
    {
      "before": ["<leader>", "c", "s"],
      "commands": ["codesearch.openCurrentFile"]
    }
  ],
  "vim.sneak": true,
  "vim.useCtrlKeys": true,
  "vim.useSystemClipboard": true,
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "after": ["<Esc>"],
      "before": [",", "."]
    },
    {
      "after": ["1", "5", "g", "k"],
      "before": ["<c-k>"]
    },
    {
      "after": ["1", "5", "g", "j"],
      "before": ["<c-j>"]
    }
  ],
  "git.detectSubmodules": false,
  "git.detectSubmodulesLimit": 9999,
  "vsicons.dontShowNewVersionMessage": true,
  "window.newWindowDimensions": "fullscreen",
  "window.openFilesInNewWindow": "default",
  "window.restoreFullscreen": true,
  "window.restoreWindows": "none",
  "window.title": "${rootName}${separator}${dirty}${activeEditorShort}",
  "window.titleBarStyle": "custom",
  "window.zoomLevel": 0,
  "workbench.activityBar.visible": false,
  "workbench.colorCustomizations": {
    "editor.background": "#000000",
    "editor.foreground": "#ffffff"
  },
  "workbench.editor.restoreViewState": false,
  "workbench.editor.showTabs": false,
  "workbench.iconTheme": "vscode-icons"
}
