{
  "[cpp]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true
  },
  "[python]": {
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit",
      "source.organizeImports": "explicit"
    },
    "editor.defaultFormatter": "charliermarsh.ruff",
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "editor.tabSize": 4
  },
  "[shellscript]": {
    "editor.defaultFormatter": "mads-hartmann.bash-ide-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[typst]": {
    "editor.formatOnSave": true
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "ansible.lightspeed.enabled": false,
  "ansible.lightspeed.suggestions.enabled": false,
  "black-formatter.args": ["--line-length=80 "],
  "chat.agent.enabled": false,
  "chat.commandCenter.enabled": false,
  "chat.disableAIFeatures": true,
  "clangd.arguments": ["-function-arg-placeholders=false"],
  "debugpy.debugJustMyCode": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.bracketPairColorization.enabled": true,
  "editor.cursorSurroundingLines": 2,
  "editor.cursorSurroundingLinesStyle": "all",
  "editor.folding": false,
  "editor.fontFamily": "'JetBrainsMono Nerd Font', 'JetBrains Mono', Consolas, 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.fontLigatures": true,
  "editor.fontSize": 12,
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "modificationsIfAvailable",
  "editor.inlayHints.enabled": "off",
  "editor.insertSpaces": true,
  "editor.lineNumbers": "off",
  "editor.linkedEditing": true,
  "editor.matchBrackets": "near",
  "editor.minimap.enabled": false,
  "editor.minimap.maxColumn": 80,
  "editor.quickSuggestions": {
    "comments": false,
    "other": true,
    "strings": false
  },
  "editor.renderFinalNewline": "off",
  "editor.rulers": [80],
  "editor.semanticHighlighting.enabled": true,
  "editor.semanticTokenColorCustomizations": {
    "rules": {
      "operator": "#FE6F88",
      "property": "#FFA87E",
      "variable": "#B7A9F1"
    }
  },
  "editor.suggest.insertMode": "replace",
  "editor.suggest.localityBonus": true,
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "[Default Dark+]": {
      "comments": "#13c100",
      "functions": "#ffa600",
      "numbers": "#fffb00",
      "textMateRules": [
        {
          "scope": ["variable"],
          "settings": {
            "foreground": "#ffffff"
          }
        },
        {
          "scope": [
            "variable.other.property",
            "variable.other.object.property"
          ],
          "settings": {
            "foreground": "#d1baec"
          }
        },
        {
          "scope": ["meta.function-call"],
          "settings": {
            "foreground": "#ffa600"
          }
        },
        {
          "scope": ["constant", "constant.language"],
          "settings": {
            "foreground": "#fffb00"
          }
        },
        {
          "scope": ["meta.attribute"],
          "settings": {
            "foreground": "#aa00ff"
          }
        },
        {
          "scope": ["keyword.operator", "punctuation", "meta.brace"],
          "settings": {
            "foreground": "#00f2ff"
          }
        },
        {
          "scope": ["keyword", "keyword.control", "keyword.other.using"],
          "settings": {
            "foreground": "#3664ff"
          }
        },
        {
          "scope": ["storage.type", "storage.class"],
          "settings": {
            "foreground": "#6d4dfd"
          }
        }
      ],
      "types": "#aa00ff"
    },
    "[Theme*]": {
      "comments": "#70ba2f",
      "functions": "#84D6E6",
      "numbers": "#d8b537",
      "strings": "#d8b537",
      "textMateRules": [
        {
          "scope": [
            "keyword.operator", // all operators
            "keyword.operator.assignment", // specifically '=' and variants
            "punctuation", // commas, semicolons, etc
            "meta.brace", // {} () []
            "punctuation.separator.period", // colons, commas in some langs
            "punctuation.separator", // colons, commas in some langs
            "punctuation.definition", // quotes, etc
            "punctuation.terminator" // semicolons
          ],
          "settings": {
            "foreground": "#FE6F88"
          }
        }
      ],
      "variables": "#B7A9F1"
    }
  },
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.wordWrap": "on",
  "editor.wrappingIndent": "deepIndent",
  "evenBetterToml.formatter.columnWidth": 80,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 1,
  "extensions.experimental.affinity": {
    "vscodevim.vim": 1
  },
  "extensions.ignoreRecommendations": false,
  "files.associations": {
    "*.pi": "python",
    "*.sdl": "sql-bigquery",
    "*.textproto": "plaintext",
    "**/nftables/**/*.conf": "nft",
    "**/roles/**/*.yml": "ansible",
    "*nftables*.conf": "nft",
    "BUILD": "bazel"
  },
  "files.autoSave": "onFocusChange",
  "files.defaultLanguage": "${activeEditorLanguage}",
  "files.enableTrash": false,
  "files.exclude": {
    "**/__pycache__ ": true,
    "**/.aider.*": true,
    "**/.ansible": true,
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.hypothesis": true,
    "**/.mypy_cache": true,
    "**/.project": true,
    "**/.pytest_cache": true,
    "**/.ruff_cache": true,
    "**/.settings": true,
    "**/.venv": true,
    "**/*.egg-info": true,
    "**/fishd.tmp.*": true
  },
  "files.hotExit": "off",
  "files.insertFinalNewline": true,
  "files.refactoring.autoSave": true,
  "files.simpleDialog.enable": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.defaultCloneDirectory": "~/repos",
  "git.detectSubmodules": false,
  "git.detectSubmodulesLimit": 9999,
  "git.path": "/usr/bin/git",
  "javascript.format.semicolons": "insert",
  "julia.enableTelemetry": false,
  "julia.symbolCacheDownload": true,
  "kilo-code.allowedCommands": [
    "git log",
    "git diff",
    "git show",
    "npm test",
    "npm install",
    "tsc"
  ],
  "kilo-code.deniedCommands": [],
  "linkme.rules": [
    {
      "pattern": "([a-z]{1,4})/([^)]+)",
      "url": "http://\\1/\\2"
    }
  ],
  "merge-conflict.autoNavigateNextConflict.enabled": true,
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "problems.showCurrentInStatus": true,
  "python.analysis.autoImportCompletions": true,
  "python.analysis.enablePytestSupport": true,
  "python.analysis.typeCheckingMode": "standard",
  "python.defaultInterpreterPath": "/bin/python",
  "python.languageServer": "Pylance",
  "redhat.telemetry.enabled": false,
  "ruff.lineLength": 80,
  "ruff.nativeServer": "on",
  "search.quickOpen.history.filterSortOrder": "recency",
  "telemetry.feedback.enabled": false,
  "terminal.integrated.commandsToSkipShell": ["language-julia.interrupt"],
  "terminal.integrated.smoothScrolling": true,
  "testing.automaticallyOpenTestResults": "openOnTestFailure",
  "tinymist.formatterPrintWidth": 80,
  "tinymist.formatterProseWrap": true,
  "typescript.format.semicolons": "insert",
  "vim.easymotion": true,
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-b>": false,
    "<C-c>": false,
    "<C-d>": false,
    "<C-e>": false,
    "<C-f>": false,
    "<C-h>": false,
    "<C-j>": true,
    "<C-k>": true,
    "<C-o>": false,
    "<C-q>": false,
    "<C-t>": false,
    "<C-v>": false,
    "<C-w>": false
  },
  "vim.highlightedyank.enable": true,
  "vim.hlsearch": false,
  "vim.incsearch": true,
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "after": ["<Esc>"],
      "before": [",", "."]
    }
  ],
  "vim.leader": "<space>",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "after": ["g", "j"],
      "before": ["j"]
    },
    {
      "after": ["g", "k"],
      "before": ["k"]
    },
    {
      "after": ["o", "<Esc>", "k"],
      "before": ["z", "j"]
    },
    {
      "after": ["O", "<Esc>", "j"],
      "before": ["z", "k"]
    },
    {
      "after": ["1", "5", "g", "k"],
      "before": ["<c-k>"]
    },
    {
      "after": ["1", "5", "g", "j"],
      "before": ["<c-j>"]
    },
    {
      "after": ["y", "$"],
      "before": ["Y"]
    },
    {
      "before": [";"],
      "commands": ["workbench.action.showCommands"]
    },
    {
      "before": ["<leader>", "t"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<leader>", "j"],
      "commands": ["workbench.action.navigateDown"]
    },
    {
      "before": ["<leader>", "k"],
      "commands": ["workbench.action.navigateUp"]
    },
    {
      "before": ["<leader>", "l"],
      "commands": ["workbench.action.navigateRight"]
    },
    {
      "before": ["<leader>", "h"],
      "commands": ["workbench.action.navigateLeft"]
    },
    {
      "before": ["<leader>", "b"],
      "commands": ["extension.vim_ctrl+o"]
    },
    {
      "before": ["<leader>", "f", "a"],
      "commands": ["testing.runAll"]
    },
    {
      "before": ["<leader>", "f", "f"],
      "commands": ["testing.reRunFailTests"]
    },
    {
      "before": ["<leader>", "s", "h"],
      "commands": ["togglehs.toggleHS"]
    },
    {
      "after": ["g", "d"],
      "before": ["<leader>", "g"],
      "doc": "this is go-to-definition; interacts with vsvim jump list better"
    },
    {
      "before": ["<leader>", "p", "c"],
      "commands": ["editor.action.revealDeclaration"]
    },
    {
      "before": ["<leader>", "q"],
      "commands": ["workbench.action.closeWindow"]
    },
    {
      "before": ["<leader>", "w"],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["<leader>", "m"],
      "commands": ["editor.action.formatDocument"]
    },
    {
      "before": ["<leader>", "p", "d"],
      "commands": ["editor.action.peekDefinition"]
    },
    {
      "before": ["<leader>", "p", "r"],
      "commands": ["editor.action.referenceSearch.trigger"]
    },
    {
      "before": ["<leader>", "c", "s"],
      "commands": ["codesearch.openCurrentFile"]
    },
    {
      "after": [],
      "before": ["u"],
      "commands": [
        {
          "args": [],
          "command": "undo"
        }
      ]
    },
    {
      "after": [],
      "before": ["<C-r>"],
      "commands": [
        {
          "args": [],
          "command": "redo"
        }
      ]
    }
  ],
  "vim.sneak": true,
  "vim.useCtrlKeys": true,
  "vim.useSystemClipboard": true,
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "after": ["<Esc>"],
      "before": [",", "."]
    },
    {
      "after": ["1", "5", "g", "k"],
      "before": ["<c-k>"]
    },
    {
      "after": ["1", "5", "g", "j"],
      "before": ["<c-j>"]
    },
    {
      "before": ["<leader>", "t"],
      "commands": ["workbench.action.quickOpen"]
    }
  ],
  "window.autoDetectHighContrast": false,
  "window.commandCenter": false,
  "window.newWindowDimensions": "fullscreen",
  "window.openFilesInNewWindow": "default",
  "window.restoreFullscreen": true,
  "window.restoreWindows": "none",
  "window.title": "${rootName}${separator}${dirty}${activeEditorShort}",
  "window.titleBarStyle": "custom",
  "workbench.activityBar.location": "bottom",
  "workbench.colorCustomizations": {
    "editor.background": "#1e1e1e",
    "editor.foreground": "#f7f6f6",
    "editor.lineHighlightBackground": "#202020b9",
    "editor.wordHighlightBackground": "#323232d3",
    "editor.wordHighlightBorder": "default",
    "editorGutter.background": "#232323"
  },
  "workbench.colorTheme": "Theme Darker",
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.showTabs": "single",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.layoutControl.enabled": false,
  "workbench.startupEditor": "none",
  "yaml.customTags": ["!vault scalar"],
  "yaml.format.proseWrap": "always"
}
